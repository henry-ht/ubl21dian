        "source": "https://github.com/henry-ht/ubl21dian"
    },
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "THE CREATIVE HENRY S.A.S.",
            "email": "soporte@thecreativehenry.app",
            "homepage": "https://thecreativehenry.app"
        }

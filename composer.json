    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 5.4",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-xmlwriter": "*",
        "nesbot/carbon": ">=2.19"
